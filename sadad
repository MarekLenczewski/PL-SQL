Create Table lands
            (id_land integer not null constraint pk_lands primary key,
            location varchar2(50),
            office_id number CONSTRAINT fk_office references office(office_id));
            
Create Table office
            (office_id int constraint pk_office_id primary key,
            office_chef_id number,
            office_emp_id number,
            address varchar2(30),
            employee_id int);
            
Create Table employees
            (employee_id int CONSTRAINT pk_employee primary key,
            names varchar2(20),
            office_chef_id number CONSTRAINT fk_chef references office(office_chef_id));
